{
  "name": "@terminus/jarvisplus",
  "version": "2.3.1",
  "description": "Convert swagger 2.0 file to typescript client",
  "main": "lib/index.js",
  "bin": "bin/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./lib",
    "prebuild": "yarn clean",
    "build": "tsc --build && npm run cptemplates",
    "cptemplates": "copyfiles -f 'src/templates/**/*.mustache' lib/templates",
    "test": "jest --coverage .",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://git.terminus.io/cnooc/jarvisplus.git"
  },
  "publishConfig": {
    "registry": "https://registry.npm.terminus.io"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "js-yaml": "^4.1.0",
    "lodash.defaults": "^4.2.0",
    "lodash.get": "^4.4.2",
    "mustache": "^4.2.0",
    "node-fetch": "^2.6.7",
    "prettier": "^2.6.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mustache": "^4.1.2",
    "@types/prettier": "^2.6.1",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/yargs": "^15.0.14",
    "copyfiles": "^2.4.1",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "swagger-schema-official": "^2.0.0-0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "keywords": [
    "swagger",
    "typescript",
    "client"
  ],
  "gitHead": "ffd1805d077030a3cb0831bcda9a8de7e296f73c"
}
