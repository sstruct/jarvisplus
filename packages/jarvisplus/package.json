    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.1",
    "@types/mustache": "^4.0.1",
    "@types/prettier": "^2.0.0",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/yargs": "^15.0.4",
    "copyfiles": "^2.3.0",
